{
  "name": "sora-js-sdk",
  "version": "2020.1.0",
  "description": "WebRTC SFU Sora JavaScript SDK",
  "main": "dist/sora.min.js",
  "module": "dist/sora.min.js",
  "types": "dist/sora.d.ts",
